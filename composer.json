{
    "name": "besitegroup/yii2-dynamicform",
    "description": "It is widget to yii2 framework to clone form elements in a nested manner, maintaining accessibility.",
    "keywords": [
        "besitegroup",
        "yii2",
        "extension",
        "widget",
        "yii2-dynamicform",
        "copy DOM element",
        "yii2 dynamic form"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://bitbucket.com/besitegroup/yii2-dynamicform"
    },
    "authors": [
        {
            "name": "BeSite",
            "email": "dev@besite.nl",
            "homepage": "http://besite.nl"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.5",
        "symfony/css-selector": "~2.8|~3.0",
        "symfony/dom-crawler": "~2.8|~3.0"
    },
    "autoload": {
        "psr-4": {
            "wbraganca\\dynamicform\\": "src"
        }
    },
    "minimum-stability": "master"
}
